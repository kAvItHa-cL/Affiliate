@model List<EEAffiliate.OrganiserSale>

@{
    ViewBag.Title = "Sales";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="header">
                        <h4 class="title">Sales Table</h4>
                        <p class="category">Here is your Transactions</p>
                    </div>
                    <div class="content table-responsive table-full-width">
                        <table class="table table-striped">
                            <thead>
                            <th>Booking ID</th>
                            <th>Event</th>
                              <th>No of Tickets</th>
                            <th>Sales</th>
                            <th>Commission</th>
                            <th>Date</th>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>

                                        <td>@item.SalesId</td>
                                        <td>@item.OrganiserEvent.Title</td>
                                        <td>@item.TicketSold</td>
                                        <td>@item.Sales</td>
                                        <td>@item.Commission</td>
                                        <td>@item.Date</td>
                                    </tr>
                                }


                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>

